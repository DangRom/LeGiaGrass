@model IEnumerable<LeGiaGrass.Models.CategoryViewModel>
<div class="side-block">
                    <h4>Chủ Đề</h4>
                    <ul class="marker-list">
                        @foreach (var category in Model)
                        {
                            <li><a href="/blog/category/@category.Alias">@category.Name</a></li>
                        }
                    </ul>
                </div>